name:                  gogol-plus-domains
version:               0.0.1
synopsis:              Google Google+ Domains API Client.
homepage:              https://github.com/brendanhay/gogol
bug-reports:           https://github.com/brendanhay/gogol/issues
license:               OtherLicense
license-file:          LICENSE
author:                Brendan Hay
maintainer:            Brendan Hay <brendan.g.hay@gmail.com>
copyright:             Copyright (c) 2015 Brendan Hay
category:              Network, Google, Cloud
build-type:            Simple
cabal-version:         >= 1.10
extra-source-files:    README.md

description:
    The Google+ API enables developers to build on top of the Google+
    platform.
    .
    /Warning:/ This is an experimental prototype/preview release which is still
    under exploratory development and not intended for public use, caveat emptor!

source-repository head
    type:     git
    location: git://github.com/brendanhay/gogol.git

library
    default-language:  Haskell2010
    hs-source-dirs:    src gen

    ghc-options:       -Wall

    exposed-modules:
          Network.Google.API.PlusDomains.Activities.Get
        , Network.Google.API.PlusDomains.Activities.Insert
        , Network.Google.API.PlusDomains.Activities.List
        , Network.Google.API.PlusDomains.Audiences.List
        , Network.Google.API.PlusDomains.Circles.AddPeople
        , Network.Google.API.PlusDomains.Circles.Get
        , Network.Google.API.PlusDomains.Circles.Insert
        , Network.Google.API.PlusDomains.Circles.List
        , Network.Google.API.PlusDomains.Circles.Patch
        , Network.Google.API.PlusDomains.Circles.Remove
        , Network.Google.API.PlusDomains.Circles.RemovePeople
        , Network.Google.API.PlusDomains.Circles.Update
        , Network.Google.API.PlusDomains.Comments.Get
        , Network.Google.API.PlusDomains.Comments.Insert
        , Network.Google.API.PlusDomains.Comments.List
        , Network.Google.API.PlusDomains.Media.Insert
        , Network.Google.API.PlusDomains.People.Get
        , Network.Google.API.PlusDomains.People.List
        , Network.Google.API.PlusDomains.People.ListByActivity
        , Network.Google.API.PlusDomains.People.ListByCircle
        , Network.Google.PlusDomains
        , Network.Google.PlusDomains.Types

    other-modules:
          Network.Google.PlusDomains.Types.Product
        , Network.Google.PlusDomains.Types.Sum

    build-depends:
          gogol-core == 0.0.1.*
        , base       >= 4.7 && < 5
